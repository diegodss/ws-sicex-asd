			<xsd:documentation>
				Tipo: Telefono AEM
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SicexTelefono" type="aem:TelefonoType"/>
		</xsd:sequence>
	</xsd:complexType>
