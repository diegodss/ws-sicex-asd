			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:boolean"/>
	</xsd:simpleType>
	<xsd:complexType name="SicexFechaHora">
		<xsd:annotation>
			<xsd:documentation>
				Tipo:Fecha AEM
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="aem:fechaHoraValida"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SicexAdjunto">
		<xsd:annotation>
			<xsd:documentation>
				Tipo:Adjunto AEM
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SicexAdjunto" type="aem:AdjuntosBinariosType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SicexRut">
		<xsd:annotation>
			<xsd:documentation>
				Tipo: Rut AEM
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="SicexRut" type="aem:runType"/>
				<xsd:element name="Pasaporte" type="aem:SicexStringSeis">
					<xsd:annotation>
						<xsd:documentation>
							Tipo:Pasaporte
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SicexCorreoElectronico">
		<xsd:annotation>
			<xsd:documentation>
				Tipo: Correo Electronico AEM
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SicexCorreoElectronico" type="aem:DireccionEmailType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SicexNombres">
		<xsd:annotation>
			<xsd:documentation>
				Tipo: Nombres y apellidos AEM
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="SicexNombres" type="aem:nombreType" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RazonSocial" type="aem:SicexStringSeis" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>
							Tipo: Razon social
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
