	<xsd:simpleType name="SicexNumericoSiete">
		<xsd:annotation>
			<xsd:documentation>
				Tipo: Decimal
				Largo Maximo: 1,4 = 9,9999
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:pattern value="[0-9]{1}.[0-9]{4}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SicexNumericoOcho">
		<xsd:annotation>
			<xsd:documentation>
				Tipo: Decimal
				Largo Maximo: 2,4 = 99,9999
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="6"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SicexNumericoNueve">
		<xsd:annotation>
			<xsd:documentation>
				Tipo: Decimal
				Largo Maximo: 5,4 = 99999,9999
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="9"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SicexNumericoDiez">
		<xsd:annotation>
			<xsd:documentation>
				Tipo: Decimal
				Largo Maximo: 10,6 =
				9999999999,999999
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="16"/>
		</xsd:restriction>
