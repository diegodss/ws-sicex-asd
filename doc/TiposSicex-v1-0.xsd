		<xsd:annotation>
			<xsd:documentation>
				Tipo: Decimal
				Largo Maximo: 18,6 =
				999999999999999999,999999
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="24"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SicexNumericoDoce">
		<xsd:annotation>
			<xsd:documentation>
				Tipo: Decimal
				Largo Maximo: 25,6 =
				9999999999999999999999999,999999
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="31"/>
		</xsd:restriction>
	</xsd:simpleType>
